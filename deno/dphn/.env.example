# Dolphin API Proxy Environment Variables
# Copy this file to .env.dphn and customize as needed

# Server Configuration
DPHN_PORT=9091
DPHN_DEFAULT_KEY=sk-dphn-key

# Model Configuration
DPHN_MODEL_NAME=Dolphin 24B
DPHN_DEFAULT_TEMPLATE=logical  # Options: logical, summary, code-beginner, code-advanced

# Upstream API
DPHN_UPSTREAM_URL=https://chat.dphn.ai/api/chat

# Features
DPHN_DEBUG_MODE=true
DPHN_DEFAULT_STREAM=true
DPHN_DASHBOARD_ENABLED=true
